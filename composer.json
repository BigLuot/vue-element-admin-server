{
  "name": "supper-happysir/base-admin",
  "description": "a basic content management system",
  "keywords": [
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.1.3",
    "barryvdh/laravel-cors": "^0.11.0",
    "doctrine/dbal": "^2.8",
    "fideloper/proxy": "^4.0",
    "jellybool/flysystem-upyun": "^1.0",
    "laravel/framework": "5.6.*",
    "laravel/tinker": "^1.0",
    "predis/predis": "^1.1",
    "prettus/l5-repository": "^2.6",
    "super-happysir/share-package": "^1.0",
    "swooletw/laravel-swoole": "^2.5",
    "symfony/psr-http-message-bridge": "^1.1",
    "tymon/jwt-auth": "1.*@rc",
    "unicodeveloper/laravel-password": "^1.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.4",
    "beyondcode/laravel-query-detector": "^1.0",
    "filp/whoops": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.13",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "orangehill/iseed": "^2.5",
    "phpunit/phpunit": "^7.0",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "*",
    "xethron/migrations-generator": "^2.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "files": [
      "app/Utils/Functions/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan ide-helper:generate"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "secure-http": true,
    "disable-tls": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  }
}
